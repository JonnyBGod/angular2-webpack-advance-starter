{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "noEmit": true,
    "noEmitHelpers": true,
    "strictNullChecks": false,
    "lib": [
      "es2015"
    ],
    "types": [],
    "baseUrl": "./",
    "paths": {
      "frameworks/*": [
        "../src/app/frameworks/*"
      ],
      "components/*": [
        "../src/app/components/*"
      ],
      "assets/*": [
        "../src/app/assets/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "platforms",
    "desktop",
    "dist",
    "./**/*.spec.ts",
    "./**/*.e2e.ts",
    "../src/**/*.spec.ts",
    "../src/**/*.e2e.ts"
  ],
  "awesomeTypescriptLoaderOptions": {
    "forkChecker": true,
    "useWebpackText": true
  },
  "angularCompilerOptions": {
    "skipMetadataEmit": true
  }
}
