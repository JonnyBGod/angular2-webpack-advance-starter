{
  "name": "angular2-webpack-advance-starter",
  "version": "1.0.0",
  "description": "An Angular 2 Webpack Starter kit featuring Angular 2 (Router, Http, Forms, Services, Tests, E2E, Coverage), Karma, Protractor, Jasmine, Istanbul, TypeScript, and Webpack by AngularClass",
  "keywords": [
    "angular2",
    "webpack",
    "typescript"
  ],
  "author": "Jo√£o Ribeiro <jonnybgod@gmail.com>",
  "homepage": "https://github.com/JonnyBGod/angular2-webpack-advance-starter",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:dev",
    "build:aot:prod": "npm run clean:dist && npm run clean:aot && webpack --config config/advance/webpack.web.prod.js  --progress --profile --bail",
    "build:aot:prod:desktop": "npm run clean:aot && webpack --config config/advance/webpack.desktop.build.renderer.js  --progress --profile --bail && npm run build:prod:desktop:main",
    "build:aot": "npm run build:aot:prod",
    "build:aot:desktop": "npm run clean:dist:desktop && npm run build:aot:prod:desktop && build -mwl --x64",
    "build:aot:desktop:linux": "npm run clean:dist && rimraf desktop/linux/* && npm run build:aot:prod:desktop && build --linux --x64",
    "build:aot:desktop:mac": "npm run clean:dist && rimraf desktop/mac/* && npm run build:aot:prod:desktop && build --macos --x64",
    "build:aot:desktop:windows": "npm run clean:dist && rimraf desktop/win/* && npm run build:aot:prod:desktop && build --win --x64",
    "build:desktop": "npm run clean:dist:desktop && npm run build:prod:desktop && build -mwl --x64",
    "build:desktop:linux": "npm run clean:dist && rimraf desktop/linux/* && npm run build:prod:desktop && build --linux --x64",
    "build:desktop:mac": "npm run clean:dist && rimraf desktop/mac/* && npm run build:prod:desktop && build --macos --x64",
    "build:desktop:windows": "npm run clean:dist && rimraf desktop/win/* && npm run build:prod:desktop && build --win --x64",
    "build:dev": "npm run clean:dist && webpack --config config/advance/webpack.web.dev.js --progress --profile",
    "build:docker": "npm run build:prod && docker build -t angular2-webpack-advance-start:latest .",
    "build:prod": "npm run clean:dist && webpack --config config/advance/webpack.web.prod.js  --progress --profile --bail",
    "build:prod:desktop": "npm run build:prod:desktop:renderer && npm run build:prod:desktop:main",
    "build:prod:desktop:main": "webpack --config config/advance/webpack.desktop.build.main.js  --progress --profile --bail",
    "build:prod:desktop:renderer": "webpack --config config/advance/webpack.desktop.build.renderer.js  --progress --profile --bail",
    "build:aot:tns": "npm run clean:tns && webpack --config config/advance/webpack.tns.prod.js  --progress --profile --bail",
    "ci:aot": "npm run test && npm run build:aot && npm run e2e",
    "ci:jit": "npm run test && npm run build:prod && npm run e2e",
    "ci:nobuild": "npm test && npm run e2e",
    "ci:testall": "npm run test && npm run build:prod && npm run e2e && npm run build:aot && npm run e2e",
    "ci:travis": "npm run test && npm run build:dev && npm run e2e:travis && npm run build:prod && npm run e2e:travis && npm run build:aot && npm run e2e:travis",
    "ci": "npm run ci:testall",
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "clean:dist:desktop": "npm run rimraf -- dist desktop",
    "clean:install": "npm set progress=false && npm i",
    "clean:tests": "npm run rimraf -- _temp/tests",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist desktop _temp compiled",
    "docker": "docker",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.+(spec|e2e).ts' ./src/",
    "e2e:live": "npm-run-all -p -r server:prod:ci protractor:live",
    "e2e:travis": "npm-run-all -p -r server:prod:ci protractor:delay",
    "e2e": "npm-run-all -p -r server:prod:ci protractor",
    "git:merge": "git fetch upstream && git merge upstream/master --allow-unrelated-histories",
    "git:merge:preview": "git fetch upstream && git merge --no-commit upstream/master --allow-unrelated-histories",
    "git:prepare": "git add . && git add -f tools/install.js && git commit -m'setup'",
    "git:setup": "git init && git remote add upstream https://github.com/JonnyBGod/angular2-webpack-advance-starter.git",
    "github-deploy:dev": "webpack --config config/advance/webpack.github-deploy.js --progress --profile  --bail --env.githubDev",
    "github-deploy:prod": "webpack --config config/advance/webpack.github-deploy.js --progress --profile  --bail --env.githubProd",
    "github-deploy:aot": "npm run clean:dist && npm run clean:aot && webpack --config config/advance/webpack.github-deploy.js --progress --profile  --bail --env.githubProd",
    "github-deploy": "npm run github-deploy:dev",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "poste2e": "npm run clean:tests",
    "poste2e:live": "npm run poste2e",
    "poste2e:travis": "npm run poste2e",
    "postinstall": "npm run webdriver:update",
    "postversion": "git push && git push --tags",
    "preclean:install": "npm run clean",
    "pree2e": "webpack --config config/advance/webpack.web.e2e.js --progress --profile && npm run webdriver:update -- --standalone",
    "pree2e:live": "npm run pree2e",
    "pree2e:travis": "npm run pree2e",
    "preversion": "npm test",
    "protractor": "protractor",
    "protractor:delay": "sleep 3 && npm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "rimraf": "rimraf",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/advance/webpack.web.dev.js --progress --profile --watch --content-base src/",
    "server:dev:desktop": "webpack --config config/advance/webpack.desktop.dev.js --progress --profile",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:prod": "static dist -a '0.0.0.0' -c false --spa",
    "server:prod:ci": "static dist -p 3000 -a '0.0.0.0' -c false --spa",
    "start": "npm run server:dev",
    "start:desktop": "npm run server:dev:desktop && NODE_ENV=development electron ./dist/main.desktop.js",
    "start:desktop:windows": "npm run server:dev:desktop && SET NODE_ENV=development && electron ./dist/main.desktop.js",
    "start:hmr": "npm run server:dev:hmr",
    "test": "npm run lint && karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@angularclass/conventions-loader": "^1.0.2",
    "@angularclass/hmr": "~1.2.2",
    "@angularclass/hmr-loader": "~3.0.2",
    "@nglibs/config": "^0.2.0-rc.4",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.2",
    "@ngrx/store": "^2.2.1",
    "@ngx-translate/core": "^6.0.1",
    "@ngx-translate/http-loader": "0.0.3",
    "angulartics2": "^1.6.4",
    "assets-webpack-plugin": "^3.4.0",
    "core-js": "^2.4.1",
    "ie-shim": "^0.1.0",
    "jasmine-core": "^2.5.2",
    "lodash-es": "^4.16.4",
    "ng2-config": "^1.1.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.2.0",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.0.0",
    "@ngrx/store-devtools": "^3.2.4",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.46",
    "@types/lodash": "^4.14.58",
    "@types/node": "^7.0.12",
    "@types/selenium-webdriver": "^3.0.0",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.0.27",
    "@types/webpack": "^2.2.12",
    "add-asset-html-webpack-plugin": "^1.0.2",
    "angular-cli": "1.0.0-beta.24",
    "angular2-template-loader": "^0.6.2",
    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "^3.1.2",
    "codelyzer": "^2.0.1",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.27.3",
    "electron": "^1.6.2",
    "electron-builder": "^16.4.2",
    "electron-debug": "^1.0.1",
    "exports-loader": "^0.6.4",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "find-root": "^1.0.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.3",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "ng-router-loader": "^2.1.0",
    "ngc-webpack": "^1.2.0",
    "ngrx-store-freeze": "^0.1.9",
    "node-sass": "^4.5.1",
    "node-static": "^0.7.9",
    "npm-run-all": "^4.0.2",
    "optimize-js-plugin": "0.0.4",
    "parse5": "^3.0.2",
    "protractor": "^5.1.1",
    "raw-loader": "0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "source-map-loader": "^0.2.0",
    "string-replace-loader": "^1.1.0",
    "style-loader": "^0.16.1",
    "to-string-loader": "^1.1.4",
    "ts-node": "^3.0.2",
    "tslib": "^1.6.0",
    "tslint": "^4.5.1",
    "tslint-loader": "^3.4.3",
    "typedoc": "^0.5.9",
    "typescript": "^2.2.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-merge": "^4.1.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "build": {
    "appId": "org.jonnybgod.angular2-webpack-advance-starter",
    "mac": {
      "category": "public.app-category.graphics-design",
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "asar": true,
    "directories": {
        "buildResources": "src/assets",
        "output": "desktop",
        "app": "dist"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JonnyBGod/angular2-webpack-advance-starter.git"
  },
  "bugs": {
    "url": "https://github.com/JonnyBGod/angular2-webpack-advance-starter/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
